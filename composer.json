{
  "name": "robinn/cache",
  "description": "Simple cache library.",
  "keywords": ["cache", "file", "redis", "memcached"],
  "license": "MIT",
  "type": "library",
  "authors": [
    {
      "name": "Róbert Kelčák",
      "email": "robo@kelcak.com",
      "homepage": "https://kelcak.com"
    }
  ],
  "minimum-stability": "stable",
  "require": {
    "php": "^7.4|^8",
    "ext-json": "*"
  },
  "suggest": {
    "ext-memcache": "Required to use the memcache cache driver.",
    "ext-memcached": "Required to use the memcached cache driver.",
    "ext-redis": "Required to use the redis cache driver."
  },
  "require-dev": {
    "phpstan/phpstan": "^1.4"
  },
  "autoload": {
    "psr-4": {
      "RobiNN\\Cache\\": "src/"
    }
  }
}
