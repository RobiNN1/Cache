{
  "name": "robinn/cache",
  "description": "Simple cache library.",
  "keywords": ["cache", "file", "memcache", "memcached", "redis"],
  "license": "MIT",
  "type": "library",
  "authors": [
    {
      "name": "Róbert Kelčák",
      "email": "robo@kelcak.com",
      "homepage": "https://kelcak.com"
    }
  ],
  "support": {
    "issues": "https://github.com/RobiNN1/Cache/issues",
    "source": "https://github.com/RobiNN1/Cache"
  },
  "minimum-stability": "dev",
  "prefer-stable": true,
  "require": {
    "php": ">=8.0",
    "ext-json": "*"
  },
  "suggest": {
    "ext-memcache": "Required to use the memcache cache driver.",
    "ext-memcached": "Required to use the memcached cache driver.",
    "ext-redis": "Required to use the redis cache driver."
  },
  "require-dev": {
    "phpstan/phpstan": "^1.4",
    "phpunit/phpunit": "^9"
  },
  "autoload": {
    "psr-4": {
      "RobiNN\\Cache\\": "src/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "Tests\\": "tests/"
    }
  },
  "scripts": {
    "phpstan": "phpstan",
    "test": "phpunit"
  }
}
